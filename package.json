{
  "name": "@haixing_hu/pinia-decorator",
  "version": "1.0.0",
  "description": "Provides class fields decorators for Pinia and vue3-class-component.",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/pinia-decorator.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/pinia-decorator/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/pinia-decorator",
  "keywords": [
    "vue",
    "vue3",
    "vue3-class-component",
    "pinia",
    "class style",
    "decorator"
  ],
  "main": "dist/pinia-decorator.cjs.js",
  "module": "dist/pinia-decorator.esm.js",
  "exports": {
    ".": {
      "import": "./dist/pinia-decorator.esm.js",
      "require": "./dist/pinia-decorator.cjs.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rvf dist",
    "lint": "eslint ./src",
    "test": "vitest run --coverage",
    "build": "cross-env NODE_ENV=production node esbuild.mjs",
    "build:dev": "cross-env NODE_ENV=development node esbuild.mjs",
    "deploy": "yarn && yarn lint && yarn test && yarn build:dev && yarn build && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "peerDependencies": {
    "@haixing_hu/vue3-class-component": "^1.1.0",
    "pinia": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@haixing_hu/eslint-config": "^1.2.0",
    "@haixing_hu/vite-plugin-babel": "^1.3.1",
    "@haixing_hu/vue3-class-component": "^1.1.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "happy-dom": "^12.9.1",
    "pinia": "^2.1.6",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue": "^3.3.4"
  },
  "packageManager": "yarn@3.6.4"
